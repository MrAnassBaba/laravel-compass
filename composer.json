{
    "name": "anass-baba/laravel-compass",
    "description": "An elegant REST assistent for the Laravel framework.",
    "keywords": [
        "laravel",
        "API",
        "testing",
        "documentation"
    ],
    "homepage": "https://github.com/MrAnassBaba/laravel-compass",
    "license": "MIT",
    "authors": [{
        "name": "David H. Sianturi",
        "email": "davidhsianturi@gmail.com"
        },{
        "name": "Anass Baba",
        "email": "anass.baba.contact@gmail.com"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "mpociot/documentarian": "dev-master"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.5",
        "laravel/sanctum": "^2.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MrAnassBaba\\Compass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrAnassBaba\\Compass\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "MrAnassBaba\\Compass\\CompassServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
